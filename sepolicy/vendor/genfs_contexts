# FastCharge HAL
genfscon sysfs /devices/platform/vendor/vendor:switch_charger/fastcharging                  u:object_r:sysfs_charger:s0

# LiveDisplay HAL
genfscon sysfs /devices/platform/soc/soc:qcom,dsi-display@17/display/lut                    u:object_r:sysfs_livedisplay:s0

# Inscreen Fingerprint HAL
genfscon sysfs /devices/platform/soc/soc:qcom,dsi-display@17/display/hbm                    u:object_r:sysfs_inscreen:s0
genfscon sysfs /devices/platform/soc/soc:steller/qos_set                                    u:object_r:sysfs_inscreen:s0

# Health HAL
genfscon sysfs /devices/platform/vendor/vendor:ui_soc_psy/power_supply/ui_soc/type          u:object_r:sysfs_power_supply:s0

# Lineage Touch HAL
genfscon sysfs /devices/platform/mx-gs/gesture_control                                      u:object_r:sysfs_touchpanel_gestures:s0

# Light HAL
genfscon sysfs /devices/platform/soc/c440000.qcom,spmi/spmi-0/spmi0-03/c440000.qcom,spmi:qcom,pmi8998@3:qcom,leds@d000/leds/breath/blink          u:object_r:sysfs_light:s0

# Vibrator HAL
genfscon sysfs /devices/platform/soc/89c000.i2c/i2c-3/3-005a/enable                    u:object_r:sysfs_vibrator:s0
genfscon sysfs /devices/platform/soc/89c000.i2c/i2c-3/3-005a/on_off                    u:object_r:sysfs_vibrator:s0

# Power Stats HAL
genfscon sysfs /devices/platform/soc/c440000.qcom,spmi/spmi-0/spmi0-02/c440000.qcom,spmi:qcom,pmi8998@2:rradc@4500/iio:device0/name          u:object_r:sysfs_iio:s0

# FOD HAL
genfscon proc /mz_info/lcd_id                    u:object_r:proc_lcd_id:s0

# AOD
genfscon sysfs /devices/platform/soc/soc:qcom,dsi-display@17/display/doze_s2                    u:object_r:sysfs_aod:s0

# CS Press
genfscon sysfs /devices/platform/soc/88c000.i2c/i2c-1/1-0026/sleep                    u:object_r:sysfs_cspress:s0
