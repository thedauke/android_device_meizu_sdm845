# FastCharge HAL
genfscon sysfs /devices/platform/vendor/vendor:switch_charger/fastcharging                  u:object_r:sysfs_charger:s0

# LiveDisplay HAL
genfscon sysfs /devices/platform/soc/soc:qcom,dsi-display@17/display/lut                    u:object_r:sysfs_livedisplay:s0

# Inscreen Fingerprint HAL
genfscon sysfs /devices/platform/soc/soc:qcom,dsi-display@17/display/hbm                    u:object_r:sysfs_inscreen:s0

# Health HAL
genfscon sysfs /devices/platform/vendor/vendor:ui_soc_psy/power_supply/ui_soc/type          u:object_r:sysfs_power_supply:s0

# Lineage Touch HAL
genfscon sysfs /devices/platform/mx-gs/gesture_control                                      u:object_r:sysfs_touchpanel_gestures:s0

# Light HAL
genfscon sysfs /devices/platform/soc/c440000.qcom,spmi/spmi-0/spmi0-03/c440000.qcom,spmi:qcom,pmi8998@3:qcom,leds@d000/leds/breath/blink          u:object_r:sysfs_light:s0
genfscon sysfs /devices/platform/soc/ae00000.qcom,mdss_mdp/backlight/panel0-backlight/brightness                                                  u:object_r:sysfs_backlight:s0
